<http://toxin.vub.be/resource/test/repeated-dose-toxicity/3>
        a                               ont:Test ;
        ont:alanine_aminotransferase    "high sgpt was noted in all animals at doses of ≥50 mg/kg bw/d." ;
        ont:aspartate_aminotransferase  "high sgot was noted in all animals at doses of ≥50 mg/kg bw/d." ;
        ont:compound                    <http://toxin.vub.be/resource/compound/triclosan> ;
        ont:erythrocyte_count           "haematology evaluations revealed decreased red blood cells and increased esr and reticulocytes in dogs at ≥50 mg/kg bw/d as well as during interim evaluation in moribund dogs." ;
        ont:guideline                   "OECD 409" ;
        ont:haematocrit                 "haematology evaluations revealed decreased  pcv in dogs at ≥50 mg/kg bw/d as well as during interim evaluation in moribund dogs." ;
        ont:haemoglobin_concentration   "haematology evaluations revealed decreased haemoglobin in dogs at ≥50 mg/kg bw/d as well as during interim evaluation in moribund dogs." ;
        ont:observation_organ_weight_organ_body_weight
                "terminal organ weight determinations revealed increased liver, pancreas and adrenal weights for 100 and 200 mg/kg bw/d animals." ;
        ont:observation_other_signs_of_toxicity
                "severity of diarrhoea was dose-related." ;
        ont:total_differential_leucocyte
                "polymorphonuclear leukocytes was observed in dogs at ≥25 mg/kg bw/d" ;
        ont:type                        "in vivo" .

